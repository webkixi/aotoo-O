//排序
.screen-box
  ss-display-flex(row nowrap, flex-start, center, center)
  border-bottom: 1px solid #ccc
  background: #fff
  position relative
.screen-item
  border-left: 1px solid #ccc
  cursor: pointer;
  flex: 1
  max-width: 50%
  margin: 0.154rem 0
  text-align: center;
  &:first-child
    border-left: none
  &:focus
    color: #2585cf
  i
    ss-iconfont()


.tree-basic-more
  font-size: .173333rem
  .tree-head
    line-height: normal
    padding-bottom: .146667rem
    color: #999
    position: relative
    em
      width: 30%;
      ss-textellipsis(20px, 1)
    p
      color: #2485cf
      width: 65%;
      text-align: right;
      ss-textellipsis(20px, 1)
      position: relative
      padding-right: 14px
      line-height: 20px
    .tree-icon-toggle
      position: relative;
      padding-right: 20px;
  >li
    @extends .border-t-distance
    &:first-child
      padding-top: 0
      margin-top: 0
    li.active,li:active
      background: #fff
      &:after
        border-color: #3b9ceb
      .destinationId
        color: #2485cf
  .property-ul      //固定生成的ul class
    padding-top: .013333rem 
    ss-list-display-com()

//综合筛选modal
.sidelayer
  height: 100%
  background: #fff
  .sidelayer-body
    width: 100%
    padding: 0.4rem 0.2rem 1rem 0.2rem
    height: 100%

  .sidelayer-foot
    ss-display-flex(row nowrap, flex-start, flex-start)
    width: 100%
    a
      border-radius: 0
      &:first-child
