//tabs切换      选择目的地页面
.tabs-lr
  ss-display-flex(row, flex-start)
  font-size: 14px
  height: 100%;
  overflow: hidden
  .itemrootCkb
    display: none
  .tabsMenus
    background: #f4f4f4
    width: 1.146667rem
    text-align: left;
    a
      color: #333
      display: block;
      padding: .16rem 0 .16rem 0.2rem;
      font-weight: 500
    ul
      color: #333
    .item
      ss-border($direction: bottom)
      &:before
        border-right: 1px solid #e3e3e3
        content: ''
        display: block
        width: .013333rem
        height: 200%
        -webkit-transform: scale(.5, .5)
        transform: scale(.5, .5)
        -webkit-transform-origin: 0 0 
        transform-origin: 0 0 
        box-sizing: border-box;
        z-index: 0;
        position: absolute
        top: 0
        right: 0
      &:hover,&.select
        background: #fff
        &:before
          content: ''
          border-right: 1px solid #fff
  .tabsBoxes
    background: #fff
    padding: 0.1rem
    color: #333
    flex: 1
    overflow: scroll
.tree-basic-tabs
  .tree-head
    padding: 0.134rem 0
    em
      font-weight: bold
      height: 16px
      overflow: hidden
      &:before
        content: ''
        border-left: .053333rem solid #2585cf
        padding-right: .053333rem
    a
      position: relative;
      padding-right: 12px;
  .property-ul
    ss-list-display-com( 4%, 4%, none, #ccc, .018667rem.093333rem, .426667rem, 1.2)
    font-size: 12px

