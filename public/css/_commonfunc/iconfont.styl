@import "../../3ds/css/iconfont.styl"
iconfont()
  +cache()
    font-family:"iconfont" !important
    font-style:normal
    -webkit-font-smoothing: antialiased
    -webkit-text-stroke-width: 0.2px
    -moz-osx-font-smoothing: grayscale
    // font-size: 16px

content(n)
  iconfont()
  content: n

.tree-toggle-icon
  &:after
    content: '\e6a3'      //向右的箭头
    position: absolute;
    top: 0
    right: 0
  &.active
    &:after
      content: '\e703'          //向下的箭头
